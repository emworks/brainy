<style>
    html,
    body {
        margin: 0;
        padding: 5px 10px;
        font-size: 14px;
        font-family: arial,sans-serif;
        color: #222;
        line-height: 1.5;
    }

    h1 {
        font-weight: 400;
        margin: 15px 0;
    }

    .tip {
        color: #c6c6c6;
        font-style: italic;
        font-size: 13px;
    }

    table {
        border-collapse: collapse;
        font-size: 12px;
        width: 100%;
    }

    th,
    td {
        border: 1px solid;
        padding: 5px;
    }
</style>

<div>
    <h1>
        <div class='tip'>
            Результаты поиска<%= q ? ' "' + q + '"' : '' %>
        </div>
    </h1>
    <table>
        <thead>
            <th>Название курса</th>
            <th>Источник</th>
            <th>Язык</th>
            <th>Рейтинг</th>
            <th>Дата</th>
        </thead>
        <tbody>
            <% for (var i = 0; i < data.length; i++) { %>
            <tr>
                <td>
                    <a href="<%= data[i].url %>" target="_blank">
                        <%= data[i].title %>
                    </a>
                </td>
                <td>
                    <%= data[i].sourceId || '-' %>
                </td>
                <td>
                    <%= 
                        data[i].lang ? ([
                            'English', 'Русский'
                        ])[data[i].lang - 1] :  '-' 
                    %>
                </td>
                <td>
                    <%= data[i].rating ? data[i].rating + '/5' : '-' %>
                </td>
                <td>
                    <%= data[i].dateFrom || '-' %>
                </td>
            </tr>
            <% } %>
        </tbody>
    </table>
</div>